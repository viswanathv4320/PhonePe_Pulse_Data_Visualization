cat >> .gitignore << 'EOF'
# OS
.DS_Store

__pycache__/
*.py[cod]

.env
my.cnf

*.pdf
*.zip
EOF

git rm --cached .DS_Store my.cnf mygeodata.zip 2>/dev/null || true
git rm -r --cached __pycache__/ 2>/dev/null || true

git add .gitignore
git commit -m "Cleanup: ignore system files, caches, secrets, and large binaries"
git push
